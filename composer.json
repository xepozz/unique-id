{
  "name": "xepozz/unique-id",
  "description": "Marking users with unique identifiers to keep the information for the further processing.",
  "keywords": [
    "yii",
    "yii3",
    "yii-framework",
    "yii-extension",
    "yii-package",
    "id",
    "unique-id",
    "user",
    "user-identification",
    "user-tracking",
    "user-tracing"
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "ramsey/uuid": "^4.7",
    "yiisoft/user": "dev-master",
    "yiisoft/yii-http": "^1.0.3"
  },
  "require-dev": {
    "httpsoft/http-message": "^1.1.1",
    "httpsoft/http-server-request": "^1.1",
    "phpunit/phpunit": "10.0.17",
    "vimeo/psalm": "^5.8",
    "yiisoft/cookies": "^1.2",
    "yiisoft/session": "^2.0",
    "yiisoft/test-support": "^3.0",
    "yiisoft/yii-debug": "dev-master"
  },
  "scripts": {
    "test": "phpunit",
    "psalm": "psalm"
  },
  "autoload": {
    "psr-4": {
      "Xepozz\\UniqueID\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xepozz\\UniqueID\\Tests\\": "tests"
    }
  },
  "extra": {
    "config-plugin-options": {
      "source-directory": "config"
    },
    "config-plugin": {
      "params": "params.php",
      "di": "di.php",
      "events": "events.php"
    }
  },
  "config": {
    "sort-packages": true
  }
}
